{
  "name": "@origyn-sa/csm",
  "version": "1.0.0-beta.0",
  "description": "JavaScript library for configuring, staging and minting Origyn NFTs from a local directory.",
  "keywords": [
    "csm",
    "config",
    "stage",
    "mint",
    "nft",
    "origyn"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "docs": "typedoc --plugin typedoc-plugin-markdown --githubPages false --out docs src/index.ts",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "npm run build && jest --config jestconfig.json",
    "version": "npm run format && git add -A src"
  },
  "devDependencies": {
    "@dfinity/agent": "^0.13.3",
    "@dfinity/identity": "^0.13.3",
    "@dfinity/principal": "^0.13.3",
    "@types/bip39": "^3.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/hdkey": "^2.0.1",
    "@types/jest": "^28.1.6",
    "@types/mime-types": "^2.1.1",
    "@types/node-fetch": "^2.6.2",
    "@types/rewire": "^2.5.28",
    "@types/sha256": "^0.2.0",
    "bip39": "^3.0.4",
    "fs-extra": "^10.1.0",
    "hdkey": "^2.0.1",
    "jest": "^28.1.3",
    "mime-types": "^2.1.35",
    "node-fetch": "^2.6.7",
    "pem-file": "^1.0.1",
    "prettier": "^2.7.1",
    "rewire": "^6.0.0",
    "sha256": "^0.2.0",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.14",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ORIGYN-SA/csm.git"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "Origyn",
  "bugs": {
    "url": "https://github.com/ORIGYN-SA/csm/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://github.com/ORIGYN-SA/csm#readme"
}
