{
  "name": "@origyn/csm",
  "version": "1.0.0-beta.6",
  "description": "JavaScript library for configuring, staging and minting Origyn NFTs from a local directory.",
  "author": "Origyn",
  "homepage": "https://github.com/ORIGYN-SA/csm#readme",
  "keywords": [
    "csm",
    "config",
    "stage",
    "mint",
    "nft",
    "origyn"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ORIGYN-SA/csm.git"
  },
  "files": [
    "lib/**/*"
  ],
  "bugs": {
    "url": "https://github.com/ORIGYN-SA/csm/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "docs": "typedoc --plugin typedoc-plugin-markdown --githubPages false --out docs src/index.ts",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "npm run build && jest",
    "version": "npm run format && git add -A src"
  },
  "dependencies": {
    "@dfinity/agent": "^0.14.1",
    "@dfinity/identity": "^0.14.1",
    "@dfinity/principal": "^0.14.1",
    "bip39": "^3.0.4",
    "fs-extra": "^10.1.0",
    "hdkey": "^2.0.1",
    "node-fetch": "^3.2.10",
    "sha256": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/fs-extra": "^9.0.13",
    "@types/hdkey": "^2.0.1",
    "@types/jest": "^29.2.3",
    "@types/node-fetch": "^2.6.2",
    "@types/rewire": "^2.5.28",
    "@types/sha256": "^0.2.0",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "mrmime": "^1.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.14",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.3"
  }
}
